@{

    @using Microsoft.Extensions.Configuration
    @inject IConfiguration Configuration

}


<style>
    * {
        box-sizing: border-box;
    }

    .BoardingPasssubheader {
        background-image: url(/assets/images/BoardingPass.jpg);
        text-align: center;
        display: flex;
        align-items: center;
        min-height: 400px;
    }

    .qr {
        display: flex;
        justify-content: center;
        position: relative;
        top: 10px;
    }

    #demo {
        margin-right: -71px;
        margin-left: -45px;
        position: relative;
    }

    div.containerr {
        max-width: 1350px;
        margin: 0 auto;
        overflow: hidden
    }

    .upcomming {
        font-size: 45px;
        text-transform: uppercase;
        border-left: 14px solid rgba(255, 235, 59, 0.78);
        padding-left: 12px;
        margin: 18px 8px;
    }

    .containerr .item {
        width: 48%;
        /*float: left;*/
        padding: 0 20px;
        background: #fff;
        overflow: hidden;
        margin: 10px
    }

    .containerr .item-right, .containerr .item-left {
        float: left;
        padding: 20px
    }

    .item-right {
        display: flex;
        flex-direction: column;
        align-items: baseline;
        justify-content: end;
        background-image: url(/assets/images/ada-logo.png);
        background-size: contain;
        background-repeat: no-repeat;
        background-position-y: 20px;
    }

    .containerr .item-right {
        padding: 44px 14px;
        margin-right: 20px;
        width: 25%;
        position: relative;
        height: 286px
    }

        .containerr .item-right .up-border, .containerr .item-right .down-border {
            padding: 14px 15px;
            background-color: #ddd;
            border-radius: 50%;
            position: absolute
        }

        .containerr .item-right .up-border {
            top: -8px;
            right: -35px;
        }

        .containerr .item-right .down-border {
            bottom: -13px;
            right: -35px;
        }

        .containerr .item-right .num {
            font-size: 34px;
            text-align: center;
            color: #111
        }

        .containerr .item-right .day, .containerr .item-left {
            color: #555;
            font-size: 20px;
            margin-bottom: 9px;
        }

    .event {
        color: #555;
        font-size: 10px;
        margin-bottom: 9px;
    }

    .containerr .item-right .day {
        text-align: center;
        font-size: 15px;
    }

    .containerr .item-left {
        width: 71%;
        padding: 34px 0px 19px 46px;
        border-left: 3px dotted #999;
    }

        .containerr .item-left .title {
            color: #111;
            font-size: 34px;
            margin-bottom: 12px
        }

        .containerr .item-left .sce {
            margin-top: 5px;
            display: block
        }

            .containerr .item-left .sce .icon, .containerr .item-left .sce p,
            .containerr .item-left .loc .icon, .containerr .item-left .loc p {
                float: left;
                word-spacing: 5px;
                letter-spacing: 1px;
                color: #888;
                margin-bottom: 10px;
            }

            .containerr .item-left .sce .icon, .containerr .item-left .loc .icon {
                margin-right: 10px;
                font-size: 20px;
                color: #666
            }

        .containerr .item-left .loc {
            display: block
        }

    .fix {
        clear: both
    }

    .containerr .item .tickets, .booked, .cancel {
        color: #fff;
        padding: 6px 14px;
        float: right;
        margin-top: 10px;
        font-size: 18px;
        border: none;
        cursor: pointer
    }

    .place-label {
        color: #d51c29;
        text-transform: uppercase;
        font-weight: 400;
        font-size: 18px;
        margin-bottom: 10px;
    }

    .place-block {
        color: #d51c29;
        text-transform: uppercase;
        font-weight: 400;
        font-size: 14px;
        margin-bottom: 10px;
    }

    .place-value {
        color: #9299a0;
        font-size: 16px;
        font-weight: 400;
    }

    .containerr .item .tickets {
        background: #777
    }

    .containerr .item .booked {
        background: #3D71E9
    }

    .containerr .item .cancel {
        background: #DF5454
    }

    .linethrough {
        text-decoration: line-through
    }

    @@media only screen and (max-width: 667px) {
        .containerr .item-right {
            width: 100%
        }

        .qr {
            display: contents;
            /*justify-content: center;*/
            position: relative;
            top: 10px;
        }
    }

    @@media only screen and (max-width: 1150px) {
        .containerr .item {
            width: 100%;
            margin-right: 20px
        }

        div.containerr {
            margin: 0 20px auto
        }
    }


