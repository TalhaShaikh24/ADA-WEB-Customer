@{
    ViewData["Title"] = "Cargo Staus";
    @using Microsoft.Extensions.Configuration
    @inject IConfiguration Configuration
}

<div class="subheader normal-bg section-padding">
    <div class="container">
        <div class="row">
            <div class="col-12 CargoStatus CargoStatus">
                <h1 key="CargoStatus" class="text-custom-white">Cargo Status</h1>

            </div>
        </div>
    </div>
</div>
<section class="section-padding ">
    <form id="cargoForm">
        <div class="container">


            <div class="row" style="justify-content: center; ">

                <div class="col-md-4">
                    <div class="form-group CargoStatus">
                        <label key="Barcode/ShipperReferenceNumber" class="fs-14 text-custom-black fw-500"> Barcode/Shipper Ref Number <span class="required">*</span> </label>
                        <input Inputkey="EnterBarcode/ShipperRefNumber" id="Barcode" type="text" name="CargoStatus" class="form-control form-control-custom" placeholder="Enter Barcode / Shipper Ref Number" />

                    </div>
                </div>
                <div class="col-md-2 mt-2  CargoStatus">
                    <button key="CargoStatusSearch" class="btn-second btn-small mt-4" type="button" id="CargoStatusSearch">Search</button>
                </div>
            </div>
            <div class="row pt-3 mt-3" id="CargoStatusFillAppend" style="justify-content: center; margin-bottom: 40px; height: 30vh; ">


            </div>
        </div>
    </form>
</section>
