    <script>
        $(document).ready(function () {

                    $.ajaxSetup({
                        xhrFields: {
                            withCredentials: true  // Automatically include cookies in every request
                        },
                        crossDomain: true  // Make sure cross-domain is allowed
                    });



            $('#preloader').delay(1000).fadeOut('slow', function () {
                $(this).hide();
            });

            if (localStorage.getItem("userData") === null) {

                $(".Sign-in").show();

                $("#MyProfile").hide();
                $("#BookingHistory").hide();



                $('.Sign-in').on('click', function () {

                    window.location.href ='@Url.Action("Login", "Authenticate")';
                })
            }
            else {

                $(".signout").show();

                $("#MyProfile").show();
                $("#BookingHistory").show();

                $('.signout').on('click', function () {

                    localStorage.removeItem('userData');
                    localStorage.removeItem('GroupMemberIds');
                    window.location.href = '@Url.Action("Index", "Home")';
                })

            }

        })
    </script>


    @await RenderSectionAsync("Scripts", required: false)
